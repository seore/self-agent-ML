{
  "env": "flappy",
  "mode": "train",
  "device": "cpu",
  "headless": true,
  "fps": 120,
  "shaping": "on",
  "config": {
    "training": {
      "gamma": 0.99,
      "lr": 0.001,
      "batch_size": 256,
      "max_memory": 100000,
      "hidden_size": 256,
      "epsilon_start": 1.0,
      "epsilon_end": 0.02,
      "epsilon_decay_games": 2000,
      "target_update_steps": 1000,
      "double_dqn": true,
      "save_every_games": 50,
      "log_every_games": 1
    },
    "runtime": {
      "fps": 120
    }
  }
}